{
  "name": "soundswiremono-monorepo",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/apis/main",
    "seed": "ts-node -r tsconfig-paths/register ./prisma/users/seed.ts",
    "clean-db": "ts-node -r tsconfig-paths/register ./prisma/users/clean-db.ts",
    "prisma:gen": "npm run prisma:gen:users && npm run prisma:gen:example-database",
    "prisma:gen:users": "prisma generate --schema prisma/users/users-schema.prisma",
    "prisma:gen:example-database": "prisma generate --schema prisma/example/example-schema.prisma",
    "prisma:migrate": "npm run prisma:migrate:users",
    "prisma:migrate:users": "prisma migrate dev --schema prisma/users/users-schema.prisma",
    "prisma:migrate:example-database": "prisma migrate dev --schema prisma/example-database/example-database-schema.prisma",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:unit": "jest --runInBand --detectOpenHandles ./apps/apis/users/src/modules/unit/unit.controller.spec.ts",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/apis/test/jest-e2e.json"
  },
  "dependencies": {
    "@casl/ability": "^6.5.0",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/microservices": "^9.4.1",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.4.2",
    "@nestjs/schedule": "^2.2.2",
    "@nestjs/throttler": "^4.1.0",
    "@nestjs/websockets": "^9.4.2",
    "@prisma/client": "^4.15.0",
    "@types/bcrypt": "^5.0.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "exceljs": "^4.3.0",
    "ftp-server": "^0.1.0",
    "ftp-srv": "^4.6.2",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "moment": "^2.29.4",
    "nest-winston": "^1.9.2",
    "nestjs-ftp": "^0.1.13",
    "node-fetch": "^2.6.11",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.2.0",
    "uuid": "^9.0.0",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.1",
    "@types/node": "18.16.12",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "prisma": "^4.15.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  }
}
